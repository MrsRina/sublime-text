    "version": "1.7.0",
    "publisher": "philipbe",
    "repository": "https://github.com/philipbelesky/gray-matter",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Gray Matter Light",
                "uiTheme": "vs",
                "path": "./Gray Matter Light-color-theme.json"
            },
            {
                "label": "Gray Matter Dark",
                "uiTheme": "vs-dark",
                "path": "./Gray Matter Dark-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "ovsx": "0.1.0-next.dacd2fd",
        "vsce": "^1.91.0"
